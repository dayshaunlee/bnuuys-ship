{
    "C_Cpp.clang_format_style": "file",
    
    "C_Cpp.codeAnalysis.clangTidy.enabled": true,

    "editor.formatOnSave": true,

    "C_Cpp.clang_format_fallbackStyle": "Google",

    "C_Cpp.default.includePath": [
        "${workspaceFolder}/**"
    ],

    "clangd.path": "clangd",
    "clangd.arguments": [
        "--compile-commands-dir=${workspaceFolder}",
        "--header-insertion=never"
    ],

    "files.associations": {
        "*.cpp": "cpp",
        "*.hpp": "cpp",
        "*.h": "cpp",
        "array": "cpp",
        "__node_handle": "cpp",
        "set": "cpp",
        "new": "cpp",
        "deque": "cpp",
        "__bit_reference": "cpp",
        "__config": "cpp",
        "__locale": "cpp",
        "__split_buffer": "cpp",
        "__verbose_abort": "cpp",
        "bitset": "cpp",
        "cctype": "cpp",
        "charconv": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "fstream": "cpp",
        "initializer_list": "cpp",
        "ios": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "locale": "cpp",
        "mutex": "cpp",
        "ostream": "cpp",
        "ratio": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "tuple": "cpp",
        "typeindex": "cpp",
        "typeinfo": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "algorithm": "cpp"
    },
    "C_Cpp.default.compilerPath": "/usr/bin/clang++"
}
